        "ext-swoole": ">= 2.0.0",
        "zendframework/zend-diactoros": "*"
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "RedPigeon\\": "src"
        }
    }
